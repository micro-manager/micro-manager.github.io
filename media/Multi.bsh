// multi channel acquistion
mmc.removeImageSynchroAll();
mmc.assignImageSynchro("Shutter");
mmc.assignImageSynchro("FilterCube");
mmc.setAutoShutter(true);

mmc.setStateLabel("FilterCube", "24 FITC/TR");
mmc.setExposure(600);
console.snapSingleImage();

mmc.setStateLabel("FilterCube", "DAPI");
