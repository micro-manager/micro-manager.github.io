mmc.setExposure(100); // ms
mmc.snapImage();
img = mmc.getImage();
width = mmc.getImageWidth();
height = mmc.getImageHeight();
byteDepth = mmc.getBytesPerPixel();
console.message("Image snapped: w=" + width + ", h=" + height + ", " + byteDepth + " byte(s) per pixel");
console.message("Image size: " + img.length + " pixels");
